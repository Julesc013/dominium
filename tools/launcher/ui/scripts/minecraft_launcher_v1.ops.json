    { "op": "set_rect", "target": { "id": "$sidebar_top" }, "x": 0, "y": 0, "w": 0, "h": 260 },
    {
      "op": "set_layout",
      "target": { "id": "$sidebar_top" },
      "dock": "TOP",
      "anchors": ["L", "T"],
      "margins": { "l": 0, "r": 0, "t": 0, "b": 8 },
      "constraints": { "min_w": 180, "min_h": 120, "max_w": -1, "max_h": -1 }
    },
    { "op": "set_container_layout", "target": { "id": "$sidebar_top" }, "mode": "STACK_COL", "params": {} },

    { "op": "create_widget", "parent": { "path": "root/main/sidebar" }, "type": "CONTAINER", "name": "sidebar_bottom", "if_exists": "reuse", "out": "$sidebar_bottom" },
