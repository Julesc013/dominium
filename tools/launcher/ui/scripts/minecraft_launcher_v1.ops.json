    { "op": "set_rect", "target": { "id": "$nav_instances" }, "x": 0, "y": 0, "w": 200, "h": 36 },
    {
      "op": "set_layout",
      "target": { "id": "$nav_instances" },
      "dock": "NONE",
      "anchors": ["L", "T"],
      "margins": { "l": 6, "r": 6, "t": 4, "b": 4 },
