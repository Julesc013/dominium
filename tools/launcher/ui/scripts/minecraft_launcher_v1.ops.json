    {
      "op": "set_layout",
      "target": { "id": "$primary" },
      "dock": "FILL",
      "anchors": ["L", "T", "R", "B"],
      "margins": { "l": 4, "r": 8, "t": 8, "b": 8 },
      "constraints": { "min_w": 300, "min_h": 200, "max_w": -1, "max_h": -1 }
