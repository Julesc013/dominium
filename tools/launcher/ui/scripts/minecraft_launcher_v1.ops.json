{
  "version": 1,
  "docname": "launcher_ui",
  "defaults": {
    "root_name": "root"
  },
  "ops": [
    { "op": "ensure_root", "name": "root", "type": "CONTAINER", "out": "$root" },
    { "op": "set_rect", "target": { "id": "$root" }, "x": 0, "y": 0, "w": 1280, "h": 720 },
    {
      "op": "set_layout",
      "target": { "id": "$root" },
      "dock": "NONE",
      "anchors": ["L", "T", "R", "B"],
      "margins": { "l": 0, "r": 0, "t": 0, "b": 0 },
      "constraints": { "min_w": 800, "min_h": 480, "max_w": -1, "max_h": -1 }
    },

    { "op": "create_widget", "parent": { "path": "root" }, "type": "CONTAINER", "name": "header", "if_exists": "reuse", "out": "$header" },
    { "op": "set_rect", "target": { "id": "$header" }, "x": 0, "y": 0, "w": 0, "h": 96 },
    {
      "op": "set_layout",
      "target": { "id": "$header" },
      "dock": "TOP",
      "anchors": ["L", "T", "R"],
      "margins": { "l": 0, "r": 0, "t": 0, "b": 0 },
      "constraints": { "min_w": 0, "min_h": 80, "max_w": -1, "max_h": -1 }
    },

    { "op": "create_widget", "parent": { "path": "root" }, "type": "CONTAINER", "name": "main", "if_exists": "reuse", "out": "$main" },
    {
      "op": "set_layout",
      "target": { "id": "$main" },
      "dock": "FILL",
      "anchors": ["L", "T", "R", "B"],
      "margins": { "l": 0, "r": 0, "t": 0, "b": 0 },
      "constraints": { "min_w": 400, "min_h": 200, "max_w": -1, "max_h": -1 }
    },

    { "op": "create_widget", "parent": { "path": "root" }, "type": "CONTAINER", "name": "footer", "if_exists": "reuse", "out": "$footer" },
    { "op": "set_rect", "target": { "id": "$footer" }, "x": 0, "y": 0, "w": 0, "h": 32 },
    {
      "op": "set_layout",
      "target": { "id": "$footer" },
      "dock": "BOTTOM",
      "anchors": ["L", "R", "B"],
      "margins": { "l": 0, "r": 0, "t": 0, "b": 0 },
      "constraints": { "min_w": 0, "min_h": 24, "max_w": -1, "max_h": -1 }
    },

    { "op": "create_widget", "parent": { "path": "root/header" }, "type": "IMAGE", "name": "header_logo", "if_exists": "reuse", "out": "$header_logo" },
    { "op": "set_rect", "target": { "id": "$header_logo" }, "x": 16, "y": 16, "w": 64, "h": 64 },
    {
      "op": "set_layout",
      "target": { "id": "$header_logo" },
      "dock": "NONE",
      "anchors": ["L", "T"],
      "margins": { "l": 0, "r": 8, "t": 0, "b": 0 },
      "constraints": { "min_w": 32, "min_h": 32, "max_w": -1, "max_h": -1 }
    },

    { "op": "create_widget", "parent": { "path": "root/header" }, "type": "STATIC_TEXT", "name": "header_title", "if_exists": "reuse", "out": "$header_title" },
    { "op": "set_rect", "target": { "id": "$header_title" }, "x": 96, "y": 20, "w": 480, "h": 24 },
    {
      "op": "set_layout",
      "target": { "id": "$header_title" },
      "dock": "NONE",
      "anchors": ["L", "T"],
      "margins": { "l": 0, "r": 0, "t": 0, "b": 0 },
      "constraints": { "min_w": 200, "min_h": 20, "max_w": -1, "max_h": -1 }
    },
    { "op": "set_prop", "target": { "id": "$header_title" }, "key": "text", "value": { "type": "string", "v": "Dominium Launcher" } },

    { "op": "create_widget", "parent": { "path": "root/header" }, "type": "STATIC_TEXT", "name": "header_version", "if_exists": "reuse", "out": "$header_version" },
    { "op": "set_rect", "target": { "id": "$header_version" }, "x": 96, "y": 50, "w": 480, "h": 18 },
    {
      "op": "set_layout",
      "target": { "id": "$header_version" },
      "dock": "NONE",
      "anchors": ["L", "T"],
      "margins": { "l": 0, "r": 0, "t": 0, "b": 0 },
      "constraints": { "min_w": 200, "min_h": 16, "max_w": -1, "max_h": -1 }
    },
    { "op": "set_prop", "target": { "id": "$header_version" }, "key": "text", "value": { "type": "string", "v": "Minecraft-style layout (structure only)" } },

