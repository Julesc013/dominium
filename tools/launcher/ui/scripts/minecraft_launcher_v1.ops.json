    {
      "op": "set_layout",
      "target": { "id": "$sidebar" },
      "dock": "LEFT",
      "anchors": ["L", "T", "B"],
      "margins": { "l": 8, "r": 8, "t": 8, "b": 8 },
      "constraints": { "min_w": 200, "min_h": 200, "max_w": 280, "max_h": -1 }
