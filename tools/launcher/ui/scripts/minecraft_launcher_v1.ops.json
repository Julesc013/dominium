      "anchors": ["L", "T"],
      "margins": { "l": 6, "r": 6, "t": 4, "b": 4 },
      "constraints": { "min_w": 180, "min_h": 32, "max_w": -1, "max_h": -1 }
    },
    { "op": "set_prop", "target": { "id": "$nav_play" }, "key": "text", "value": { "type": "string", "v": "Play" } },
    { "op": "bind_event", "target": { "id": "$nav_play" }, "event": "on_click", "action": "launcher.nav.play" },

