      "anchors": ["L", "B"],
      "margins": { "l": 0, "r": 0, "t": 8, "b": 0 },
      "constraints": { "min_w": 180, "min_h": 80, "max_w": -1, "max_h": -1 }
    },
    { "op": "set_container_layout", "target": { "id": "$sidebar_bottom" }, "mode": "STACK_COL", "params": {} },

    { "op": "create_widget", "parent": { "path": "root/main/sidebar/sidebar_top" }, "type": "BUTTON", "name": "nav_play", "if_exists": "reuse", "out": "$nav_play" },
    { "op": "set_rect", "target": { "id": "$nav_play" }, "x": 0, "y": 0, "w": 200, "h": 36 },
