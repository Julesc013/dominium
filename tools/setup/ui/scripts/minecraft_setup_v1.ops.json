{
  "version": 1,
  "docname": "setup_ui",
  "defaults": {
    "root_name": "root"
  },
  "ops": [
    { "op": "ensure_root", "name": "root", "type": "CONTAINER", "out": "$root" },
    { "op": "set_rect", "target": { "id": "$root" }, "x": 0, "y": 0, "w": 1280, "h": 720 },
    {
      "op": "set_layout",
      "target": { "id": "$root" },
      "dock": "NONE",
      "anchors": ["L", "T", "R", "B"],
      "margins": { "l": 0, "r": 0, "t": 0, "b": 0 },
      "constraints": { "min_w": 800, "min_h": 480, "max_w": -1, "max_h": -1 }
    },

    { "op": "create_widget", "parent": { "path": "root" }, "type": "CONTAINER", "name": "header", "if_exists": "reuse", "out": "$header" },
    { "op": "set_rect", "target": { "id": "$header" }, "x": 0, "y": 0, "w": 0, "h": 80 },
    {
      "op": "set_layout",
      "target": { "id": "$header" },
      "dock": "TOP",
      "anchors": ["L", "T", "R"],
      "margins": { "l": 0, "r": 0, "t": 0, "b": 0 },
      "constraints": { "min_w": 0, "min_h": 64, "max_w": -1, "max_h": -1 }
    },

    { "op": "create_widget", "parent": { "path": "root" }, "type": "CONTAINER", "name": "body", "if_exists": "reuse", "out": "$body" },
    {
      "op": "set_layout",
      "target": { "id": "$body" },
      "dock": "FILL",
      "anchors": ["L", "T", "R", "B"],
      "margins": { "l": 0, "r": 0, "t": 0, "b": 0 },
      "constraints": { "min_w": 300, "min_h": 240, "max_w": -1, "max_h": -1 }
    },

    { "op": "create_widget", "parent": { "path": "root" }, "type": "CONTAINER", "name": "footer", "if_exists": "reuse", "out": "$footer" },
    { "op": "set_rect", "target": { "id": "$footer" }, "x": 0, "y": 0, "w": 0, "h": 56 },
    {
      "op": "set_layout",
      "target": { "id": "$footer" },
      "dock": "BOTTOM",
      "anchors": ["L", "R", "B"],
      "margins": { "l": 0, "r": 0, "t": 0, "b": 0 },
      "constraints": { "min_w": 0, "min_h": 44, "max_w": -1, "max_h": -1 }
    },

    { "op": "create_widget", "parent": { "path": "root/header" }, "type": "IMAGE", "name": "header_logo", "if_exists": "reuse", "out": "$header_logo" },
    { "op": "set_rect", "target": { "id": "$header_logo" }, "x": 16, "y": 12, "w": 48, "h": 48 },
    {
      "op": "set_layout",
      "target": { "id": "$header_logo" },
      "dock": "NONE",
      "anchors": ["L", "T"],
      "margins": { "l": 0, "r": 8, "t": 0, "b": 0 },
      "constraints": { "min_w": 32, "min_h": 32, "max_w": -1, "max_h": -1 }
    },

    { "op": "create_widget", "parent": { "path": "root/header" }, "type": "STATIC_TEXT", "name": "header_title", "if_exists": "reuse", "out": "$header_title" },
    { "op": "set_rect", "target": { "id": "$header_title" }, "x": 72, "y": 16, "w": 520, "h": 22 },
    {
      "op": "set_layout",
      "target": { "id": "$header_title" },
      "dock": "NONE",
      "anchors": ["L", "T"],
      "margins": { "l": 0, "r": 0, "t": 0, "b": 0 },
      "constraints": { "min_w": 240, "min_h": 18, "max_w": -1, "max_h": -1 }
    },
    { "op": "set_prop", "target": { "id": "$header_title" }, "key": "text", "value": { "type": "string", "v": "Dominium Setup" } },

    { "op": "create_widget", "parent": { "path": "root/header" }, "type": "STATIC_TEXT", "name": "header_subtitle", "if_exists": "reuse", "out": "$header_subtitle" },
    { "op": "set_rect", "target": { "id": "$header_subtitle" }, "x": 72, "y": 40, "w": 620, "h": 18 },
    {
      "op": "set_layout",
      "target": { "id": "$header_subtitle" },
      "dock": "NONE",
      "anchors": ["L", "T"],
      "margins": { "l": 0, "r": 0, "t": 0, "b": 0 },
      "constraints": { "min_w": 240, "min_h": 16, "max_w": -1, "max_h": -1 }
    },
    { "op": "set_prop", "target": { "id": "$header_subtitle" }, "key": "text", "value": { "type": "string", "v": "Minecraft-style wizard (structure only)" } },

    { "op": "create_widget", "parent": { "path": "root/body" }, "type": "TABS", "name": "body_tabs", "if_exists": "reuse", "out": "$tabs" },
