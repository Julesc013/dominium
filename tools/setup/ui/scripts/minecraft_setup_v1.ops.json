      "anchors": ["L", "T", "R", "B"],
      "margins": { "l": 8, "r": 8, "t": 8, "b": 8 },
      "constraints": { "min_w": 300, "min_h": 240, "max_w": -1, "max_h": -1 }
    },
    { "op": "set_prop", "target": { "id": "$tabs" }, "key": "tabs.placement", "value": { "type": "string", "v": "top" } },
    { "op": "set_prop", "target": { "id": "$tabs" }, "key": "tabs.selected_index", "value": { "type": "int", "v": 0 } },

    { "op": "create_widget", "parent": { "path": "root/body/body_tabs" }, "type": "TAB_PAGE", "name": "page_welcome", "if_exists": "reuse", "out": "$page_welcome" },
    {
      "op": "set_layout",
      "target": { "id": "$page_welcome" },
      "dock": "FILL",
      "anchors": ["L", "T", "R", "B"],
      "margins": { "l": 0, "r": 0, "t": 0, "b": 0 },
      "constraints": { "min_w": 240, "min_h": 200, "max_w": -1, "max_h": -1 }
    },
    { "op": "set_prop", "target": { "id": "$page_welcome" }, "key": "tab.title", "value": { "type": "string", "v": "Welcome" } },

    { "op": "create_widget", "parent": { "path": "root/body/body_tabs" }, "type": "TAB_PAGE", "name": "page_path", "if_exists": "reuse", "out": "$page_path" },
    {
      "op": "set_layout",
      "target": { "id": "$page_path" },
      "dock": "FILL",
      "anchors": ["L", "T", "R", "B"],
      "margins": { "l": 0, "r": 0, "t": 0, "b": 0 },
      "constraints": { "min_w": 240, "min_h": 200, "max_w": -1, "max_h": -1 }
    },
    { "op": "set_prop", "target": { "id": "$page_path" }, "key": "tab.title", "value": { "type": "string", "v": "Install Location" } },

    { "op": "create_widget", "parent": { "path": "root/body/body_tabs" }, "type": "TAB_PAGE", "name": "page_components", "if_exists": "reuse", "out": "$page_components" },
    {
      "op": "set_layout",
      "target": { "id": "$page_components" },
      "dock": "FILL",
      "anchors": ["L", "T", "R", "B"],
      "margins": { "l": 0, "r": 0, "t": 0, "b": 0 },
      "constraints": { "min_w": 240, "min_h": 200, "max_w": -1, "max_h": -1 }
