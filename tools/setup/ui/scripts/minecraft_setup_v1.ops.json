      "op": "set_layout",
      "target": { "id": "$path_edit" },
      "dock": "NONE",
      "anchors": ["L", "T"],
      "margins": { "l": 4, "r": 4, "t": 2, "b": 2 },
      "constraints": { "min_w": 260, "min_h": 20, "max_w": -1, "max_h": -1 }
