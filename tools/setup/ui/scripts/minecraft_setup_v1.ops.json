      "op": "set_layout",
      "target": { "id": "$page_finish" },
      "dock": "FILL",
      "anchors": ["L", "T", "R", "B"],
      "margins": { "l": 0, "r": 0, "t": 0, "b": 0 },
      "constraints": { "min_w": 240, "min_h": 200, "max_w": -1, "max_h": -1 }
    },
    { "op": "set_prop", "target": { "id": "$page_finish" }, "key": "tab.title", "value": { "type": "string", "v": "Finish" } },

