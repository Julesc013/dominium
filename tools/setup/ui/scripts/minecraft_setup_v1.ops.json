    { "op": "set_container_layout", "target": { "id": "$components_group_core" }, "mode": "STACK_COL", "params": {} },

    { "op": "create_widget", "parent": { "path": "root/body/body_tabs/page_components/components_content/components_group_core" }, "type": "CHECKBOX", "name": "components_core_game", "if_exists": "reuse", "out": "$components_core_game" },
